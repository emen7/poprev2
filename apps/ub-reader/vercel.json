{
  "version": 2,
  "buildCommand": "cd ../.. && npx turbo run build --filter=ub-reader...",
  "installCommand": "npm install --legacy-peer-deps",
  "outputDirectory": ".next",
  "headers": [
    {
      "source": "/(.*)",
      "headers": [
        {
          "key": "Cache-Control",
          "value": "no-store, no-cache, must-revalidate, proxy-revalidate"
        }
      ]
    }
  ],
  "trailingSlash": false
}
